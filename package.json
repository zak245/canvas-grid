{
    "name": "@grid-engine/core",
    "version": "0.1.0",
    "description": "High-performance, opinionated grid engine with Canvas rendering, built-in cell types, and React integration",
    "type": "module",
    "main": "./dist/index.cjs",
    "module": "./dist/index.js",
    "types": "./dist/index.d.ts",
    "exports": {
        ".": {
            "types": "./dist/index.d.ts",
            "import": "./dist/index.js",
            "require": "./dist/index.cjs"
        },
        "./react": {
            "types": "./dist/react.d.ts",
            "import": "./dist/react.js",
            "require": "./dist/react.cjs"
        },
        "./core": {
            "types": "./dist/core.d.ts",
            "import": "./dist/core.js",
            "require": "./dist/core.cjs"
        }
    },
    "files": [
        "dist",
        "README.md",
        "LICENSE"
    ],
    "sideEffects": false,
    "scripts": {
    "dev": "vite",
    "build": "npm run build:lib",
    "build:playground": "tsc && vite build",
    "build:lib": "tsup",
        "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
        "preview": "vite preview",
        "storybook": "storybook dev -p 6006",
        "build-storybook": "storybook build",
        "prepublishOnly": "npm run build:lib"
    },
    "keywords": [
        "grid",
        "spreadsheet",
        "canvas",
        "react",
        "data-grid",
        "table",
        "virtualization",
        "high-performance"
    ],
    "author": "",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": ""
    },
    "peerDependencies": {
        "react": ">=17.0.0",
        "react-dom": ">=17.0.0"
    },
    "peerDependenciesMeta": {
        "react": {
            "optional": true
        },
        "react-dom": {
            "optional": true
        }
    },
    "dependencies": {
        "clsx": "^2.1.0",
        "immer": "^10.0.3",
        "lucide-react": "^0.344.0",
        "tailwind-merge": "^2.2.1",
        "zustand": "^4.5.0"
    },
    "devDependencies": {
        "@storybook/addon-essentials": "^8.6.14",
        "@storybook/react-vite": "^8.6.14",
        "@types/react": "^18.2.56",
        "@types/react-dom": "^18.2.19",
        "@vitejs/plugin-react": "^4.2.1",
        "autoprefixer": "^10.4.18",
        "postcss": "^8.4.35",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "storybook": "^8.6.14",
        "tailwindcss": "^3.4.1",
        "tsup": "^8.5.1",
        "typescript": "^5.2.2",
        "vite": "^5.1.4"
    }
}
